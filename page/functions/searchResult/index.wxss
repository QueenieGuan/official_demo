.main {
  display: flex;
  flex-flow: row wrap;
}
.list_item {
  background: #ffffff;
  width: 100%;
  margin-bottom: 20px;
  padding: 5px 10px;
}
.big_section {
  display: flex;
  justify-content: space-between;
}
.small_section {
  display: flex;
  align-items: center;
}
.kindergarten_icon {
  font-size: 24px;
}
.name {
  font-size: 16px;
  font-weight: 700;
  font-family: KaiTi;
  margin-left: 10px;
  height: 24px;
  width: 150px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.price_icon {
  font-size: 20px;
  color: #1E90FF;
}
.star_icon {
  font-size: 20px;
  color: #FFD700;
}
.score_icon {
  font-size: 20px;
  color: #FFD700;
}
.route_icon {
  font-size: 24px;
  color: #00BFFF;
}
.text {
  margin-left: 5px;
}
.no_data {
  display: flex;
  flex-direction: column;
  justify-content: center;
  text-align: center;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
}
.warning_icon {
  font-size: 48px;
  color: #00BFFF;
}
.warning_text {
  font-size: 16px;
  color: #696969;
}
